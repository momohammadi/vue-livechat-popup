{
  "name": "vue-livechat-popup",
  "type": "module",
  "version": "1.0.1",
  "description": "This simple and practical Vue.js plugin helps you effortlessly create a beautiful popup for your live chat section on your website using messaging services like WhatsApp, Telegram, Messenger, etc., without the need for any server or account.",
  "author": "Morteza Saeed mohammadi <m.mohammadi721@gmail.com> (https://github.com/momohammadi)",
  "main": "dist/vue-livechat-popup.cjs.js",
  "browser": "dist/vue-livechat-popup.es.js",
  "module": "dist/vue-livechat-popup.es.js",
  "unpkg": "dist/vue-livechat-popup.iife.js",
  "jsdelivr": "dist/vue-livechat-popup.iife.js",
  "assetsPublicPath": "src/assets",
  "style": "dist/style.css",
  "files": [
    "dist/*",
    "src/assets/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "demo": "cd demo && vite",
    "demo:build": "cd demo && vite build",
    "demo:publish": "cp ./demo/dist/index.html ./demo/dist/200.html && surge ./demo/dist https://vue-livechat-popup.surge.sh/",
    "publish": "git push --tags && npm run build && npm publish"
  },
  "dependencies": {
    "@vue-a11y/focus-loop": "^1.1.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "stylus": "^0.64.0",
    "vite": "^6.0.0",
    "vue-router": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momohammadi/vue-livechat-popup.git"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "livechat",
    "social",
    "popup",
    "chat",
    "marketing"
  ]
}
